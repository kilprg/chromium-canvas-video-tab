{
  "manifest_version": 3,
  "name": "Canvas videos in new tab",
  "version": "1.1",
  "description": "Injects links to open Canvas videos in new tabs.",
  "content_scripts": [
    {
      "matches": [
        "https://*.instructure.com/*",
        "https://*.kaltura.nordu.net/*",
        "https://play.ki.se/*",
        "https://*.cloudfront.net/*"
      ],
      "all_frames": true,
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" }
}
